{
  "name": "rds-client",
  "version": "0.2.0",
  "description": "Rds Client",
  "author": "Dmytro Petrenko",
  "license": "ISC",
  "types": "./src/index.ts",
  "main": "./dist/index.js",
  "files": [
    "./dist/index.js"
  ],
  "scripts": {
    "prestart": "yarn install",
    "prebuild": "yarn install",
    "prelint:js": "yarn install",
    "test": "jest --config=./scripts/jest/config.js",
    "lint:ts": "./node_modules/.bin/tsc --skipLibCheck --noEmit",
    "lint:js": "./node_modules/.bin/eslint --max-warnings 0 -f table --ext .ts src/ && yarn lint:ts",
    "build": "[ -d ./dist ] && rm -R ./dist; cross-env NODE_ENV=production webpack --config=./scripts/webpack/prod.js && rm -rf ./dist/*.txt"
  },
  "dependencies": {
    "nanoid": "3.1.30",
    "redis": "4.0.1"
  },
  "devDependencies": {
    "@types/jest": "27.0.3",
    "@types/node": "14.18.2",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.30.0",
    "cross-env": "7.0.3",
    "eslint": "7.28.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "12.3.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-loader": "3.0.4",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "27.4.5",
    "jest-sonar": "0.2.12",
    "prettier": "2.5.1",
    "ts-jest": "27.1.2",
    "ts-loader": "9.2.6",
    "typescript": "4.3.2",
    "webpack": "5.65.0",
    "webpack-cli": "4.9.1",
    "webpack-merge": "5.7.3",
    "webpackbar": "5.0.2"
  }
}
